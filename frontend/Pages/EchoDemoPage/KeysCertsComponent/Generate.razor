@inject HttpClient http

<MudButton Disabled="@_processing_generatekeys" OnClick="GenerateKeys" Variant="Variant.Filled" Color="Color.Primary">
    <MudIcon Size="Size.Small" Class="mr-1 ms-n2" Icon="@Icons.Material.Filled.AddCircle" Title="Add" />
    <MudText>Generate Keys</MudText>
</MudButton>

@code {
    private bool _processing_generatekeys = false;

    private async Task GenerateKeys()
    {
        _processing_generatekeys = true;
        await http.GetAsync("/api/keys/generate");
        _processing_generatekeys = false;
    }
}